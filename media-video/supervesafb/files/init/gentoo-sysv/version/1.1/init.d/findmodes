# $Header: $


service_name="supervesafb findmodes"

start () 
{
    ebegin "Starting ${service_name}"
    
    /sbin/fbsetmoded findmodes
    eend ${retval}
}

stop () 
{
    ebegin "Stopping ${service_name}"
    
    eend ${retval}
}


# Local Variables:
# mode: sh
# End:

